  },
  "main": "ResponsiveImageView.js",
  "scripts": {
    "clean": "rimraf coverage",
    "format": "prettier --write \"**/*.{js,json,md,yml}\"",
    "lint": "eslint .",
    "precommit": "lint-staged",
