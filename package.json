{
  "name": "react-native-responsive-image-view",
  "version": "3.0.0",
  "private": false,
  "description": "React Native component for scaling an Image within the parent View",
  "keywords": [
    "aspect-ratio",
    "fit",
    "flexible",
    "hook",
    "image",
    "react",
    "react-native",
    "responsive",
    "view"
  ],
  "homepage": "https://npm.im/react-native-responsive-image-view",
  "bugs": {
    "url": "https://github.com/wKovacs64/react-native-responsive-image-view/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wKovacs64/react-native-responsive-image-view.git"
  },
  "source": "src/index.tsx",
  "files": [
    "src",
    "dist/commonjs",
    "dist/module",
    "dist/typescript/src",
    "!**/__fixtures__",
    "!**/__tests__"
  ],
  "main": "dist/commonjs/index.js",
  "module": "dist/module/index.js",
  "types": "dist/typescript/src/index.d.ts",
  "react-native": "src/index.tsx",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf coverage dist",
    "changeset": "changeset",
    "changeset:version": "changeset version && pnpm install",
    "changeset:publish": "changeset publish",
    "format": "oxfmt --write .",
    "format:check": "oxfmt --check .",
    "lint": "eslint .",
    "prepack": "bob build",
    "prepublishOnly": "run-p --silent lint test",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "7.29.0",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@changesets/changelog-github": "0.5.2",
    "@changesets/cli": "2.29.8",
    "@eslint/js": "10.0.1",
    "@testing-library/react-native": "13.3.3",
    "@types/jest": "30.0.0",
    "@types/react": "19.2.14",
    "@types/react-test-renderer": "19.1.0",
    "babel-jest": "30.2.0",
    "babel-plugin-syntax-hermes-parser": "0.33.3",
    "doctoc": "2.3.0",
    "eslint": "9.39.3",
    "eslint-plugin-jest": "29.15.0",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-testing-library": "7.16.0",
    "jest": "30.2.0",
    "metro-react-native-babel-preset": "0.77.0",
    "npm-run-all2": "8.0.4",
    "oxfmt": "0.35.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-native": "0.84.0",
    "react-native-builder-bob": "0.40.18",
    "react-test-renderer": "19.2.4",
    "rimraf": "6.1.3",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.1"
  },
  "peerDependencies": {
    "react": ">=16.8.3",
    "react-native": ">=0.59.0"
  },
  "packageManager": "pnpm@10.30.1",
  "react-native-builder-bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
